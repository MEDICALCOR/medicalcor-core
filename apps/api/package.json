{
  "name": "@medicalcor/api",
  "version": "0.1.0",
  "private": true,
  "description": "Fastify webhook gateway for MedicalCor",
  "type": "module",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "tsx watch src/app.ts",
    "start": "node dist/app.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/helmet": "^12.0.1",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/sensible": "^6.0.0",
    "@fastify/swagger": "^9.6.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@medicalcor/core": "workspace:*",
    "@medicalcor/types": "workspace:*",
    "@trigger.dev/sdk": "^3.1.0",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/pg": "^8.11.0",
    "ioredis": "^5.8.2",
    "pg": "^8.16.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
