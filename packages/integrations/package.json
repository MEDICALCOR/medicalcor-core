{
  "name": "@medicalcor/integrations",
  "version": "0.1.0",
  "private": true,
  "description": "Third-party service integrations for MedicalCor",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./adapters/payment": {
      "types": "./dist/adapters/payment/index.d.ts",
      "import": "./dist/adapters/payment/index.js"
    },
    "./adapters/voice": {
      "types": "./dist/adapters/voice/index.d.ts",
      "import": "./dist/adapters/voice/index.js"
    },
    "./adapters/ai": {
      "types": "./dist/adapters/ai/index.d.ts",
      "import": "./dist/adapters/ai/index.js"
    },
    "./adapters/crm": {
      "types": "./dist/adapters/crm/index.d.ts",
      "import": "./dist/adapters/crm/index.js"
    },
    "./hubspot": {
      "types": "./dist/hubspot.d.ts",
      "import": "./dist/hubspot.js"
    },
    "./whatsapp": {
      "types": "./dist/whatsapp.d.ts",
      "import": "./dist/whatsapp.js"
    },
    "./openai": {
      "types": "./dist/openai.d.ts",
      "import": "./dist/openai.js"
    },
    "./vapi": {
      "types": "./dist/vapi.d.ts",
      "import": "./dist/vapi.js"
    },
    "./__mocks__/handlers": {
      "import": "./src/__mocks__/handlers.ts"
    },
    "./__mocks__/server": {
      "import": "./src/__mocks__/server.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "clean": "rm -rf dist .turbo tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@medicalcor/core": "workspace:*",
    "@medicalcor/domain": "workspace:*",
    "@medicalcor/types": "workspace:*",
    "openai": "^4.70.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
